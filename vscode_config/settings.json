{
  "window.zoomLevel": 1,
  "terminal.integrated.inheritEnv": false,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "typescript.tsdk": "./client/node_modules/typescript/lib",
  "eslint.validate": ["javascript"],
  "go.toolsManagement.autoUpdate": true,
  "python.defaultInterpreterPath": "/usr/local/bin/python3.10",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // "vscode-neovim.neovimClean": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/nvim-macos-x86_64/bin/nvim",
  // "vscode-neovim.neovimExecutablePaths.darwin": "/opt/nvim-macos-arm64/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/ericqian/dot-files-and-configs/nvim_config/init.vim",
  "jupyter.askForKernelRestart": false
}
